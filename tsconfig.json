{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015", "dom", "es5", "scripthost"],
    "jsx": "react",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noEmit": false,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "outDir": "./build"
  },
  "include": ["./src/**/*.ts", "./src/**/*.tsx"],
  "exclude": ["./src/**/*.scss"]
}
